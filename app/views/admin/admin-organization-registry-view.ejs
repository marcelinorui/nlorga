<div class="col-lg-12">
	<div class="list-group list-group-horizontal">
		<% for(var i = 0; i < registry.length; i++){ %>
			<div class="col-lg-3 list-group-item">
				<span class="<%- registry[i].profession%>-icon"></span>
				<% if( registry[i].commander != '' ) { %>
					<span class="<%- registry[i].commander%>-icon"></span>
				<% } %>
				<% if( registry[i].havebanner == true ) { %>
					<span class="banner-icon"></span>
				<% } %>
				<% if( registry[i].havefood == true ) { %>
					<span class="food-icon"></span>
				<% } %>
				
				<span class="pull-right"><%- registry[i].displayname != '' ? registry[i].displayname : registry[i].username %></span>
			</div>
		<% } %>
	</div>
</div>