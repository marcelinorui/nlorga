<div class="col-lg-12">
	<div class="list-group list-group-horizontal">
		<% for(var i = 0; i < registrys.length; i++){ %>
			<div class="col-lg-3 list-group-item">
				<% if( registrys[i].commander != '' ) { %>
					<span class="<%- registrys[i].commander%>-icon"></span>
				<% } %>
				<% if( registrys[i].havebanner == true ) { %>
					<span class="banner-icon"></span>
				<% } %>
				<% if( registrys[i].havefood == true ) { %>
					<span class="food-icon"></span>
				<% } %>
				<span class="<%- registrys[i].profession%>-icon"></span>
				<span class="user"><%- registrys[i].displayname != '' ? registrys[i].displayname : registrys[i].username %></span>
			</div>
		<% } %>
	</div>			
</div>