	<table class="table table-striped table-hover">
		<thead>
			<tr>
				<th>#</th>
				<th></th>
				<th></th>
				<th></th>
				<th>Created</th>
				<th>
					<div class="btn-group pull-right">
						<a href="/admin/organization/create" role="button" class="btn btn-primary btn-sm" data-toggle="tooltip" title="New Account">
							<span class="glyphicon glyphicon-plus"></span> New
						</a>
					</div>
				</th>
			</tr>
		</thead>
		<tbody>
			<% for(var i = 0; i < organizations.data.length; i++ ){ %>
				<tr>
					<td><%- organizations.data[i].idorganization %></td>
					<td><%- organizations.data[i].status %></td>
					<td><%- organizations.data[i].title %></td>
					<td><%- organizations.data[i].createddate %>
					<td><%- organizations.data[i].updateddate %>
					<td>
						<a href="/admin/organization/<%-organizations.data[i].idorganization%>/edit" role="button" class="btn btn-success btn-sm" title="Edit" data-toggle="tooltip" >
